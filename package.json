{
    "name": "my-stretching",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "lint:fix": "next lint --fix",
        "prepare": "husky",
        "typecheck": "tsc -p tsconfig.json --noEmit",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:watch": "vitest",
        "verify": "yarn lint && yarn typecheck && yarn test:coverage"
    },
    "dependencies": {
        "next": "15.5.10",
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^6.8.0",
        "@testing-library/react": "^16.3.0",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "@vitejs/plugin-react": "^5.0.3",
        "@vitest/coverage-v8": "3.2.4",
        "eslint": "^9",
        "eslint-config-next": "15.4.10",
        "husky": "^9.1.7",
        "jsdom": "^27.0.0",
        "lint-staged": "^16.2.0",
        "typescript": "^5",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.2.4"
    },
    "lint-staged": {
        "*.{ts,tsx,js,jsx}": [
            "eslint --fix"
        ]
    }
}
